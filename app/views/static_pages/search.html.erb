<div class="container search-form">
  <%= form_for @search_form, as: 'search', url: :search, html: {method: :get} do |f| %>
  <%= f.text_field :name %>

  <%= f.submit "検索" %>

  <% end %>

</div>

<%= render partial: 'shared/items_list',
  locals: {items: @items,
  div_tag_name: 'search_results'} %>
