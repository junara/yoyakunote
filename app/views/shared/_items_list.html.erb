<div class="container items-list-partial <%= div_tag_name %>">
  <% items.each do |item| %>
  <div class="col-sm-4">
    <div class="">
      <%= link_to item.name, item_path(uuid: item.uuid) %>
    </div>
    <div>
      <%= button_to '使用相談', matchings_path(matching: {item_id: item.id, user_id: session[:user_id], match_type: 'innovation'}) %>
    </div>

  </div>
  <% end %>
</div>
